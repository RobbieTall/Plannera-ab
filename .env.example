# Pull the live connection string via `vercel env pull .env` or copy it from
# Vercel → Project → Settings → Environment Variables → DATABASE_URL.
DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE"
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="replace-with-strong-secret"
# Optional override for magic-link base URL.
# If not set, the app will fall back to VERCEL_URL or the incoming request origin.
APP_URL="http://localhost:3000"
MAGIC_LINK_SECRET="replace-with-strong-secret"
RESEND_API_KEY=""
EMAIL_SERVER_HOST="smtp.example.com"
EMAIL_SERVER_PORT="587"
EMAIL_SERVER_USER="apikey"
EMAIL_SERVER_PASSWORD="super-secret"
EMAIL_FROM="Plannera <auth@plannera.ai>"
AUTH_TRUST_HOST=true
NSW_PROPERTY_API_URL="https://api.planningportal.nsw.gov.au/property"
NSW_PROPERTY_API_KEY="replace-with-nsw-key"
NSW_WATER_API_URL="https://api.planningportal.nsw.gov.au/water"
NSW_WATER_API_KEY="replace-with-nsw-key"
NSW_TRADES_API_URL="https://api.planningportal.nsw.gov.au/trades"
NSW_TRADES_API_KEY="replace-with-nsw-key"
BLOB_READ_WRITE_TOKEN="vercel-blob-token"
